{
    "name": "oslint",
    "version": "0.1.2",
    "description": "Open Source Lint",
    "main": "dist/main.js",
    "scripts": {
        "start": "node dist/main.js",
        "compile": "npm run build-frontend && npm run tsc",
        "tslint": "tslint *.ts lib/*.ts lib/*/*.ts lib/*/*/*.ts",
        "tsc": "rm -rf dist && tsc",
        "build-frontend": "rm -rf public && parcel build web/index.html -d public/",
        "watch-frontend": "rm -rf public && parcel watch web/index.html -d public/"
    },
    "browserslist": [
        "last 1 Chrome versions"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/angrykoala/oslint.git"
    },
    "files": [
        "dist"
    ],
    "author": "angrykoala <angrykoala@outlook.es>",
    "license": "GPL-3.0",
    "keywords": [
        "oslint",
        "open-source",
        "lint"
    ],
    "bugs": {
        "url": "https://github.com/angrykoala/oslint/issues"
    },
    "homepage": "https://oslint.angrykoala.xyz",
    "devDependencies": {
        "@types/chai": "^4.2.7",
        "@types/express": "^4.17.2",
        "@types/mocha": "^5.2.7",
        "@types/morgan": "^1.7.37",
        "@types/node": "^13.1.1",
        "@vue/component-compiler-utils": "^3.1.0",
        "bulma": "^0.8.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "mocha": "^6.2.2",
        "nyc": "^15.0.0",
        "parcel-bundler": "^1.12.4",
        "sass": "^1.24.0",
        "ts-node": "^8.5.4",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4",
        "vue": "^2.6.11",
        "vue-router": "^3.1.3",
        "vue-template-compiler": "^2.6.11",
        "vuex": "^3.1.2"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "morgan": "^1.9.1",
        "vue-hot-reload-api": "^2.3.4"
    }
}
